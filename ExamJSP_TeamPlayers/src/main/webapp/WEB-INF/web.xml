<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.example.examjsp_teamplayers.controller.LoginController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TeamController</servlet-name>
        <servlet-class>com.example.examjsp_teamplayers.controller.TeamController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TeamController</servlet-name>
        <url-pattern>/TeamController</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>PlayerController</servlet-name>
        <servlet-class>com.example.examjsp_teamplayers.controller.PlayerController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PlayerController</servlet-name>
        <url-pattern>/PlayerController</url-pattern>
    </servlet-mapping>


</web-app>